{
  "name": "taktigrynerychat",
  "version": "0.3.3",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "*** Build ***": "",
    "build:ui": "nx build ui",
    "build:jungle-ui": "nx build jungle-ui",
    "*** Analyze ***": "",
    "analyze:ui": "nx build ui --stats-json && webpack-bundle-analyzer dist/apps/ui/stats.json",
    "analyze:jungle-ui": "nx build jungle-ui --stats-json && webpack-bundle-analyzer dist/libs/jungle-ui/stats.json",
    "*** Test ***": "",
    "test-coverage:ui": "nx test ui --coverage --config ./apps/ui/jest.config.ts",
    "test-coverage:ui:changed": "nx test ui --coverage --onlyChanged --config ./apps/ui/jest.config.ts",
    "test-coverage:jungle-ui": "nx test jungle-ui --coverage --config ./libs/jungle-ui/jest.config.ts",
    "test-coverage:jungle-ui:changed": "nx test jungle-ui --coverage --onlyChanged --config ./libs/jungle-ui/jest.config.ts",
    "*** Lint ***": "",
    "lint:ui": "nx lint ui",
    "lint:jungle-ui": "nx lint jungle-ui",
    "*** Storybook ***": "",
    "storybook:jungle-ui": "nx storybook jungle-ui",
    "build:storybook:jungle-ui": "nx build-storybook jungle-ui",
    "*** CI ***": "",
    "affected:test:coverage": "nx affected --target=test --coverage --parallel",
    "affected:build": "nx affected --target=build --parallel",
    "affected:lint": "nx affected --target=lint --parallel --exclude=jungle-ui-e2e,ui-e2e",
    "*** Publish ***": "",
    "copy-readme": "copy libs/jungle-ui/README.md dist/libs/jungle-ui",
    "copy-license": "copy libs/jungle-ui/LICENSE dist/libs/jungle-ui",
    "copy-changelog": "copy libs/jungle-ui/Changelog.md dist/libs/jungle-ui",
    "copy-files": "run-s copy-readme copy-license",
    "build:jungle-ui:package": "npm run build:jungle-ui && cd dist/libs/jungle-ui && npm pack",
    "publish:jungle-ui": "npm publish --access public dist/libs/jungle-ui",
    "*** Versioning ***": "",
    "bump-version:major": "npm version major --force && cd libs/jungle-ui && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/jungle-ui && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/jungle-ui && npm version patch --force",
    "pre-publish:minor": "run-s bump-version:minor build:jungle-ui:package",
    "pre-publish:major": "run-s bump-version:major build:jungle-ui:package",
    "pre-publish:patch": "run-s bump-version:patch build:jungle-ui:package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nrwl/angular": "14.1.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@nrwl/cli": "14.1.7",
    "@nrwl/cypress": "14.1.7",
    "@nrwl/eslint-plugin-nx": "14.1.7",
    "@nrwl/jest": "14.1.7",
    "@nrwl/linter": "14.1.7",
    "@nrwl/nx-cloud": "14.0.5",
    "@nrwl/storybook": "14.1.7",
    "@nrwl/workspace": "14.1.7",
    "@storybook/addon-essentials": "~6.4.12",
    "@storybook/angular": "~6.4.12",
    "@storybook/builder-webpack5": "~6.4.12",
    "@storybook/core-server": "~6.4.12",
    "@storybook/manager-webpack5": "~6.4.12",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "nx": "14.1.7",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "~4.6.2",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
